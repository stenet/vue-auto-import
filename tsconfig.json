{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "auto-imports.d.ts",
    "components.d.ts",
    "typed-router.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "~icons/*": ["./nodes-modules/@iconify/*"]
    },
    "types": [
      "unplugin-icons/types/vue"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
